<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.redis01.dao.SysRoleDao">
    <!--开启自定义redis缓存-->
    <cache-ref namespace="com.study.redis01.dao.SysUserDao"/>

    <update id="updateById" parameterType="com.study.redis01.entity.SysRole">
        update SYS_ROLE set ROLE_NAME = #{roleName} where ID = #{id}
    </update>
</mapper>